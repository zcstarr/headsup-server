{
 "name": "headsup-server",
 "version": "0.0.0",
 "private": false,
 "publishConfig": {
  "access": "restricted"
 },
 "author": {
  "name": "Zane",
  "email": "zane@ships.gold"
 },
 "main": "build/index.js",
 "scripts": {
  "gen:types": "npx --no-install @open-rpc/typings -d ./openrpc.json  --output-ts ./src/generated",
  "gen:schema-types": "node scripts/generate-types",
  "build:code": "tsc",
  "build:publish-client": "npm run gen:client && npm run build:client",
  "build": "npm run build:client && npm run install:client && npm run build:code",
  "gen:client": "npx --no-install @open-rpc/generator generate -c ./open-rpc-generator-config.json",
  "build:client": "npm run gen:client && cd ./generated-client/client/typescript && npm --force install && npx --no-install tsc",
  "install:client": "npm run clean:assets; npm pack generated-client/client/typescript/ && mv *.tgz headsup-server-client.tgz && npm install --no-save headsup-server-client.tgz",
  "clean:assets": "rm *.tgz",
  "clean": "npm uninstall @shipsgold/ships-client",
  "build:all": "npm run gen:types && npm run build:code",
  "test": "npm run build && jest --coverage",
  "start": "npm install && npm run build:publish-client && npm run install:client && npm run build:code && node build/src/index.js"
 },
 "dependencies": {
  "@erc725/erc725.js": "^0.14.4",
  "@open-rpc/client-js": "^1.6.3",
  "@open-rpc/meta-schema": "^1.14.2",
  "@open-rpc/schema-utils-js": "^1.14.3",
  "@open-rpc/server-js": "^1.9.0",
  "@open-rpc/typings": "^1.11.3",
  "@pinata/sdk": "^1.1.26",
  "body-parser": "^1.19.1",
  "connect": "^3.7.0",
  "cors": "^2.8.5",
  "express": "^4.18.1",
  "formidable": "^2.0.1",
  "fs-extra": "^10.1.0",
  "image-size": "^1.0.2",
  "postgres": "^2.0.0-beta.11",
  "sha3": "^2.1.4"
 },
 "devDependencies": {
  "@json-schema-tools/dereferencer": "^1.5.3",
  "@json-schema-tools/reference-resolver": "^1.2.1",
  "@json-schema-tools/transpiler": "^1.10.2",
  "@open-rpc/generator": "^1.18.12",
  "@open-rpc/semantic-release-plugin": "^1.0.2",
  "@qiwi/semantic-release-gh-pages-plugin": "^5.1.1",
  "@semantic-release/changelog": "^5.0.1",
  "@semantic-release/commit-analyzer": "^8.0.1",
  "@semantic-release/exec": "^5.0.0",
  "@semantic-release/git": "^9.0.1",
  "@semantic-release/github": "^7.2.3",
  "@semantic-release/npm": "^7.1.3",
  "@semantic-release/release-notes-generator": "^9.0.3",
  "@types/body-parser": "^1.19.2",
  "@types/cors": "^2.8.12",
  "@types/express": "^4.17.13",
  "@types/formidable": "^2.0.5",
  "@types/fs-extra": "^9.0.13",
  "@types/jest": "^27.4.0",
  "@typescript-eslint/eslint-plugin": "^4.31.2",
  "@typescript-eslint/parser": "^4.31.2",
  "eslint-config-airbnb": "^18.2.1",
  "eslint-config-prettier": "^8.3.0",
  "eslint-plugin-import": "^2.24.2",
  "jest": "^27.4.5",
  "prettier": "^2.4.1",
  "rimraf": "^3.0.2",
  "source-map-support": "^0.5.19",
  "ts-jest": "^27.0.1",
  "typescript": "^4.2.4"
 }
}
